# Dev.to Mirror Environment Variables
# Copy this file to .env and fill in your values

# Your Dev.to username (required)
DEVTO_USERNAME=your-username

# Your GitHub username for Pages URLs (required)
GH_USERNAME=your-username

# Optional: Force full regeneration (true/false)
FORCE_FULL_REGEN=false

# Optional: Use mock data instead of API calls (true/false)
# Set to true to test generation pipeline without hitting Dev.to API
VALIDATION_MODE=false
