# Dev.to Mirror Environment Variables
# Copy this file to .env and fill in your values

# Your Dev.to username (required)
DEVTO_USERNAME=your-username

# Your GitHub Pages repository in format "username/repo-name" (required)
PAGES_REPO=your-username/devto-mirror

# Optional: Force full regeneration (true/false)
FORCE_FULL_REGEN=false

# Optional: Enable validation mode for testing (true/false)
VALIDATION_MODE=false